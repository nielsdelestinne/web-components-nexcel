{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/desktop/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/packages/desktop",
        "compiler": "babel",
        "index": "packages/desktop/src/index.html",
        "baseHref": "/",
        "main": "packages/desktop/src/main.ts",
        "polyfills": "packages/desktop/src/polyfills.ts",
        "tsConfig": "packages/desktop/tsconfig.app.json",
        "assets": [
          "packages/desktop/src/favicon.ico",
          "packages/desktop/src/assets"
        ],
        "styles": ["packages/desktop/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "packages/desktop/src/environments/environment.ts",
              "with": "packages/desktop/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "desktop:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "desktop:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/desktop/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/desktop"],
      "options": {
        "jestConfig": "packages/desktop/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
