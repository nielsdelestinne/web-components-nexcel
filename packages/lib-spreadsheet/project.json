{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/lib-spreadsheet/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/lib-spreadsheet",
        "main": "packages/lib-spreadsheet/src/index.ts",
        "tsConfig": "packages/lib-spreadsheet/tsconfig.lib.json",
        "assets": ["packages/lib-spreadsheet/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/lib-spreadsheet/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/lib-spreadsheet"],
      "options": {
        "jestConfig": "packages/lib-spreadsheet/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
